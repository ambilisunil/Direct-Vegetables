<style>
body {
  font-family: "Lato", sans-serif;
  background-image:url("../bck2.jpg");
}

.sidenav {
  height: 100%;
  width: 260px;
  position: fixed;
  z-index: 1;
  top: 0;
  left: 0;
  background-color: rgb(3, 87, 3);
  overflow-x: hidden;
  padding-top: 20px;
  border-style: solid;
  border-top:  rgb(3, 87, 3);
  box-shadow: green;
  
  
}

.sidenav a {
  padding: 6px 6px 6px 32px;
  text-decoration: none;
  font-size: 25px;
  color: #e9dada;
  display: block;
  border-style:solid;
  width:80%;
  
}

.sidenav a:hover {
  color: #f1f1f1;
  background-color:rgb(6, 179, 6) ;
}

.main {
  margin-left: 200px; /* Same as the width of the sidenav */
}

@media screen and (max-height: 450px) {
  .sidenav {padding-top: 15px;}
  .sidenav a {font-size: 18px;}
}


h1{
